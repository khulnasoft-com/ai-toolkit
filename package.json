{
  "private": true,
  "name": "ai-repo",
  "scripts": {
    "build:docker-base": "docker build -t ai-toolkit -f ./tools/docker-builder/Dockerfile .",
    "build": "turbo build",
    "build:packages": "turbo --filter './packages/**' build",
    "changeset": "changeset",
    "clean": "turbo clean",
    "clean:install": "pnpm clean ; pnpm i",
    "clean:build": "pnpm clean ; pnpm i && pnpm build:packages",
    "clean:nodeModules": "find . -name node_modules -type d -exec rm -rf {} \\;",
    "clean:next": "find . -name .next -type d -exec rm -rf {} \\;",
    "clean:nuxt": "find . -name .nuxt -type d -exec rm -rf {} \\;",
    "clean:dist": "find . -name dist -type d -exec rm -rf {} \\;",
    "clean:turbo": "find . -name .turbo -type d -exec rm -rf {} \\;",
    "dev": "turbo dev --no-cache  --concurrency 17 --continue --filter './examples/*' --filter './examples/*'",
    "lint": "turbo lint",
    "lint:fix": "biome lint --write && pnpm eslint '**/*.vue' --fix",
    "format:packages": "pnpm synopkg fix-mismatches --types 'prod,dev' || true && pnpm synopkg set-semver-ranges && pnpm install -r",
    "format:check": "pnpm prettier --check . && biome format",
    "format": "prettier --write . && biome format --write",
    "lint:check": "biome lint --diagnostic-level=error",
    "lint:vue": "pnpm eslint '**/*.vue'",
    "prepare": "husky install",
    "prettier-check": "prettier --check \"**/*.{js,ts,tsx,md,mdx,svelte}\"",
    "type-check": "turbo type-check",
    "prettier-fix": "prettier --write \"**/*.{js,ts,tsx,md,mdx,svelte}\"",
    "publint": "turbo publint",
    "test": "turbo test",
    "ci:release": "turbo clean && turbo build && changeset publish",
    "ci:version": "changeset version && node .github/scripts/cleanup-examples-changesets.mjs && pnpm install --no-frozen-lockfile",
    "clean-examples": "node .github/scripts/cleanup-examples-changesets.mjs && pnpm install --no-frozen-lockfile"
  },
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.27.10",
    "@playwright/test": "^1.44.1",
    "eslint": "^9.18.0",
    "eslint-config-khulnasoft-ai": "workspace:*",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "next": "15.0.0-canary.23",
    "playwright": "^1.44.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.2",
    "react": "19.0.0-rc-cc1ec60d0d-20240607",
    "react-dom": "19.0.0-rc-cc1ec60d0d-20240607",
    "turbo": "^2.4.4",
    "typescript": "^5.6.3",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": "^18.0.0 || ^20.0.0 || ^22.0.0"
  },
  "homepage": "https://sdk.khulnasoft.com/docs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khulnasoft/ai.git"
  },
  "license": "Apache License",
  "bugs": {
    "url": "https://github.com/khulnasoft/ai/issues"
  },
  "keywords": [
    "ai"
  ],
  "packageManager": "pnpm@9.12.3",
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "all",
    "plugins": [
      "prettier-plugin-svelte"
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "dependencies": {
    "synopkg": "^13.0.4"
  },
  "remarkConfig": {
    "plugins": [
      "remark-validate-links",
      [
        "remark-lint-no-dead-urls",
        {
          "skipUrlPatterns": [
            "http://localhost"
          ]
        }
      ]
    ]
  }
}
